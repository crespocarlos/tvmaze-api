{
    "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "object-literal-sort-keys": false,
      "interface-name": false,
      "member-ordering": false,
      "prefer-for-of": false,
      "variable-name": false,
      "no-console": false,
    },
    "linterOptions": {
      "exclude": [
        "src/pb/**/*",
        "src/admin/generated/**/*"
      ]
    }
  }
  
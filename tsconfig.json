{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "lib": [
        "es2015",
        "es2016",
        "es2017",
        "es2018",
        "esnext.asynciterable"
      ],
      "allowJs": true,
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": "./",
      "importHelpers": true,
      "strict": true,
      "strictNullChecks": false,
      "moduleResolution": "node",
      "typeRoots": [
        "node_modules/@types"
      ],
      "esModuleInterop": true
    },
    "include": [
      "src/**/*"
    ]
  }
  